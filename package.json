{
  "name": "miro-latex",
  "version": "0.0.1",
  "description": "miro latex integration",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently --kill-others npm:vite npm:nodemon",
    "nodemon": "nodemon -r dotenv/config server.js",
    "vite": "vite",
    "build": "vite build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "nodemonConfig": {
    "ignore": [
      "front/*",
      "dist/*"
    ],
    "watch": [
      ".",
      ".env"
    ]
  },
  "author": "Grady Weyenberg",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.2.1",
    "dotenv": "^16.0.1",
    "http-proxy-middleware": "^2.0.6",
    "mirotone": "^2.0.9",
    "nodemon": "^2.0.16",
    "vite": "^2.9.9"
  },
  "dependencies": {
    "express": "^4.18.1",
    "ioredis": "^5.0.5",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": "16.x"
  }
}
